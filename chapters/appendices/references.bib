  shorttitle = {Silicon {{Phthalocyanines}} for N-{{Type Organic Thin-Film Transistors}}},
  author = {King, Benjamin and Melville, Owen A. and Rice, Nicole A. and Kashani, Somayeh and Tonnel{\'e}, Claire and Raboui, Hasan and Swaraj, Sufal and Grant, Trevor M. and McAfee, Terry and Bender, Timothy P. and Ade, Harald and Castet, Fr{\'e}d{\'e}ric and Muccioli, Luca and Lessard, Beno{\^i}t H.},
  year = {2021},
