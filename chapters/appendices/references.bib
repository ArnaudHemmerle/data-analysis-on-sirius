@article{elhaitami2018,
  title = {Inorganic Mixed Phase Templated by a Fatty Acid Monolayer at the Air--Water Interface: The {{Mn}} and {{Mg}} Case},
  shorttitle = {Inorganic Mixed Phase Templated by a Fatty Acid Monolayer at the Air--Water Interface},
  author = {El Haitami, Alae and Goldmann, Michel and Fontaine, Philippe and Faur{\'e}, Marie-Claude and Cantin, Sophie},
  year = {2018},
  month = feb,
  journal = {Physical Chemistry Chemical Physics},
  volume = {20},
  number = {9},
  pages = {6629--6637},
  publisher = {The Royal Society of Chemistry},
