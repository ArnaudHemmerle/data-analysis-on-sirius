@article{nath2024,
  title = {Understanding the {{Heterointerfaces}} in {{Perovskite Solar Cells}} via {{Hole Selective Layer Surface Functionalization}}},
  author = {Nath, Bidisha and Behera, Sushant K and Kumar, Jeykishan and Hemmerle, Arnaud and Fontaine, Philippe and Ramamurthy, Praveen C and Mahapatra, Debiprosad Roy and Hegde, Gopalkrishna},
