  title = {Electrolytes at Interfaces: Accessing the First Nanometers Using {{X-ray}} Standing Waves},
  shorttitle = {Electrolytes at Interfaces},
  author = {ben Jabrallah, Soumaya and Malloggi, Florent and Belloni, Luc and Girard, Luc and Novikov, Dmitri and Mocuta, Cristian and Thiaudi{\`e}re, Dominique and Daillant, Jean},
