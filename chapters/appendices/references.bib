@article{labiod2022,
  ids = {labiod2022a},
  title = {Photo-Degradation in Bulk Heterojunction Organic Solar Cells Using a Fullerene or a Non-Fullerene Derivative Electron Acceptor},
  author = {Labiod, Amina and Ibraikulov, Olzhas A. and {Dabos-Seignon}, Sylvie and Ferry, St{\'e}phanie and Heinrich, Beno{\^i}t and M{\'e}ry, St{\'e}phane and Fall, Sadiara and Tchognia Nkuissi, Herv{\'e} J. and Heiser, Thomas and Cabanetos, Cl{\'e}ment and Leclerc, Nicolas and L{\'e}v{\^e}que, et Patrick},
  year = {2022},
  month = aug,
  journal = {Organic Electronics},
  volume = {107},
  pages = {106549},
  issn = {1566-1199},
  doi = {10.1016/j.orgel.2022.106549},
  urldate = {2022-05-19},
  abstract = {The use of Non-Fullerene Acceptors (NFAs) in the active layer of organic solar cells (OSCs) has made it possible to exceed 18\% conversion efficiency. However, OSCs still present stability issues under operational conditions that need to be surpassed for their industrialization. In this work, we investigated binary and ternary blends to examine their efficiency and their stability as active layers of OSCs. We used a fluorinated polymer (PF2) as an electron donor and two different electron acceptors, a fullerene derivative (PC71BM) and a NFA (EH-IDTBR). We demonstrated that using EH-IDTBR instead of PC71BM leads to a decrease in efficiency attributed to the low out-of-plane electron mobility measured in the blend. However, using EH-IDTBR as single electron-acceptor significantly enhanced the OSCs stability under continuous illumination. Ternary blends were tested to reach simultaneously a high efficiency and a long-term stability. The best efficiency/stability compromise appeared to be when using EH-IDTBR only as electron-acceptor. We identified changes in the main charge-carrier recombination mechanism in photo-degraded devices from bimolecular in low EH-IDTBR content blends to trap-assisted in high EH-IDTBR ones. Finally, the blend morphology at a nanometer scale appeared as stable in high EH-IDTBR content blends while photo-degradation impacted significantly the morphology of the low EH-IDTBR content blend.},
  langid = {english},
  keywords = {Organic photovoltaics,Photo-degradation,Ternary blend},
  file = {/Users/hemmerle/Zotero/storage/GYVSHMV7/Labiod et al. - 2022 - Photo-degradation in bulk heterojunction organic s.pdf;/Users/hemmerle/Zotero/storage/RZYYSAZU/S1566119922001215.html}
}

@article{lashkov2021,
  ids = {lashkov2021a},
  title = {Modulation {{Doping}} for {{Threshold Voltage Control}} in {{Organic Field-Effect Transistors}}},
  author = {Lashkov, Ilia and Krechan, Kevin and Ortstein, Katrin and Talnack, Felix and Wang, Shu-Jen and Mannsfeld, Stefan C. B. and Kleemann, Hans and Leo, Karl},
  year = {2021},
