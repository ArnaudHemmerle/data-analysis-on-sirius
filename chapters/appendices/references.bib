@article{galerne2021,
  title = {Spatially {{Addressed Supramolecular Nanowires}}: {{A Full Structural Characterization}} by {{GIWAXS}}},
  shorttitle = {Spatially {{Addressed Supramolecular Nanowires}}},
  author = {Galerne, Melodie and Heinrich, Beno{\^i}t and Hemmerle, Arnaud and Fontaine, Philippe and Giuseppone, Nicolas and K{\'e}kicheff, Patrick},
